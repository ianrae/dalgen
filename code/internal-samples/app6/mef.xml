<?xml version="1.0"?>
<entities orm="ebean"> 

<entity name="User">
	<field>@Id Long id</field>
	<field>@Required String name</field>

	<query>find_by_name</query>

	<codegen what="entity" extend="true" />
	<codegen what="model" extend="false" />
	<codegen what="dao_interface" extend="false" />
	<codegen what="dao_mock" extend="true" />
	<codegen what="dao_real" extend="true" />
</entity>



</entities>
