fielddecl(type, name, bigName, setName) ::= <<

		<type> <name>;
		
		public <type> get<bigName>() {
			return this.entity.<name>;
		}
		public void <setName><bigName>(<type> val) {
			this.entity.<name> = val;
		}
>>



classdecl(type, name) ::= <<public class <name> extends Model
{
		private <type> entity;
		public void initEntity(<type> entity)
		{
			this.entity = entity;
		}
		
		//getters and setters
		
>>

endclassdecl() ::= <<
}
>>

sampleloop(items) ::= "<items:{it|<it.id>: <it.lastName>, <it.firstName>\n}>"